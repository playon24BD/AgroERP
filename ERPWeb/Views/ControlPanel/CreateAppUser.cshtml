                    txtPassword.val(res.Password);
                    txtConfirmPassword.val(res.ConfirmPassword);
                    ddlOrganizationName.val(res.OrganizationId);
                    role = res.RoleId;
                    branch = res.BranchId;
                    var Divisions = res.Division.split(',');
                    ddlDivisionName.val(Divisions);


                    ddlOrganizationName.trigger("change");
                    ddlBranchName.val(res.BranchId);
        
                    ddlRoleName.val(res.RoleId);
                    alert("from data Role Id:" + ddlRoleName.val(role));
          
                    
                    chkStatestatus.prop("checked", res.IsActive);
                    chkStatestatusRole.prop("checked", res.IsRoleActive);
                    
                }
            }).fail(function (error) {
                console.log(error);
            })
