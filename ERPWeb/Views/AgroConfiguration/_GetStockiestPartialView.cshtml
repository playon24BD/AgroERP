@model IEnumerable<ERPBO.Agriculture.ViewModels.StockiestInfoViewModel>

@{
    ViewBag.Title = "_GetStockiestPartialView";
    var SL = 0;
    var privilege = (ERPBO.Common.UserPrivilege)ViewBag.UserPrivilege;
}
<div class="row">
    <div class="col-md-12" style="overflow-y:scroll">
        <table class="table table-bordered table-striped table-hover table-sm text-sm" id="tblStockiestData">
            <thead class="btn-dark">
                <tr class="text-center">
                    <th style="vertical-align:middle">#SL</th>
                    <th style="vertical-align:middle">Area Name</th>
                    <th style="vertical-align:middle">Territory Name</th>
                    <th style="vertical-align:middle">Stockiest Name</th>
                    <th style="vertical-align:middle">Number</th>
                    <th style="vertical-align:middle">Status</th>
                    <th style="vertical-align:middle">Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    SL = SL + 1;
                <tr>

                    <td class="text-center text-bold">@SL</td>
                    <td class="hide">@item.StockiestId</td>
                    <td class="hide">@item.TerritoryId</td>
                    <td class="hide">@item.AreaId</td>
                    <td class="text-center">@item.AreaName</td>
                    <td class="text-center">@item.TerritoryName</td>
                    <td class="text-center">@item.StockiestName</td>
                    <td class="text-center">@item.StockiestPhoneNumber</td>
                    <td class="text-center">@item.Status</td>
                    <td class="text-center hide">@item.StockiestMail</td>
                    <td class="text-center hide">@item.StockiestTradeLicense</td>
                    <td class="text-center hide">@item.StockiestNID</td>
                    <td class="text-center hide">@item.StockiestContactPerson</td>
                    <td class="text-center hide">@item.StockiestContactPersonPHNumber</td>
                    <td class="text-center hide">@item.CreditLimit</td>
                    <td class="text-center">
                        <a href="#" class="btn btn-warning btn-sm data-edit-item-StockiestId" style="line-height:1" data-edit-item-Territory="@item.StockiestId"><i class="fa fa-pen"></i> Edit</a>
                    </td>
                </tr>
                }


            </tbody>
        </table>
    </div>

</div>

