
@model IEnumerable<ERPBO.Agriculture.ViewModels.ReturnRawMaterialViewModel>

@{
    ViewBag.Title = "_GetReturnReportListView";
    int sl = 0;
   
}


<table class="table table-bordered table-sm table-striped table-responsive-lg text-sm" id="tblReturnReportList">
    <thead class="btn-dark">
        <tr class="text-center text-bold">
            <th style="vertical-align:middle">#SL</th>
            <th style="vertical-align:middle" class="hide"></th>
            <th style="vertical-align:middle" class="hide"></th>
            <th style="vertical-align:middle">Entry Date</th>
            <th style="vertical-align:middle">Raw Material Name</th>
            <th style="vertical-align:middle">Quantity</th>
            <th style="vertical-align:middle">Return Type</th>
            <th style="vertical-align:middle">Status</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            sl = sl + 1;
            <tr>
                <td class="text-center text-bold">@sl</td>
                <td class="hide">@item.ReturnRawMaterialId</td>
                <td class="text-center">@item.EntryDate.Value.ToString("yyyy-MMM-dd")</td>
                <td class="text-center">@item.RawMaterialName</td>
                <td class="text-center">@item.Quantity</td>
                <td class="text-center">@item.ReturnType</td>
                <td class="text-center">@item.Status</td>
             
            </tr>
        }
        @if (Model.Count() == 0)
        {
            <tr class="text-center text-bold">
                <td colspan="11">No data found in the table</td>
            </tr>
        }
    </tbody>
</table>



